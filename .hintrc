{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "typescript-config/consistent-casing": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 15.8"
  ]
}